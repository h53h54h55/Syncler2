        "enabled": true,
        "languages": [
            "en"
        ],
        "base_url": "https://nyaa.si",
        "fallback_urls": [],
        "response_type": "text",
        "movie": {
            "query": "/?f=0&c=0_0&q={query}&s=seeders&o=desc",
            "keywords": "{title} {year}"
        },
        "episode": {
            "query": "/?f=0&c=0_0&q={query}&s=seeders&o=desc",
            "keywords": "{title} {absoluteNumber}"
        },
        "season": {
            "query": "/?f=0&c=0_0&q={query}&s=seeders&o=desc",
